{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "/public/dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "esModuleInterop": true
  },
  "include": [
    "src"
  ]
}